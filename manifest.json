{
  "manifest_version": 3,
  "name": "GSC URL Insights",
  "description": "Show Google Search Console metrics for the current page.",
  "version": "0.1.0",
  "permissions": ["tabs", "identity", "storage"],
  "host_permissions": ["https://www.googleapis.com/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
